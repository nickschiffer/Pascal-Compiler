t1 := 0;
t2 := 1;
nextTerm := 0;
n := 0;

n = 10000; //Number to go up to in sequence

//Displays the first two terms which is always 0 and 1
printf("Fibonacci Series is %d, %d", t1, t2);

nextTerm = t1 + t2;

while (nextTerm <= n) {
    printf(", %d", nextTerm);
    t1 = t2;
    t2 = nextTerm;
    nextTerm = t1 + t2;
}