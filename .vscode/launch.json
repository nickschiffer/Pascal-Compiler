{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/GoGo",
            "args": ["test1.gogo"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "type": "antlr-debug",
            "request": "launch",
            "name": "Run Parser",
            "input": "test.gogo",
            "grammar": "GoGo.g4",
            "startRule": "prog",
            "visualParseTree": true
        },
        {
            "name": "antlr4-GoGo",
            "type": "antlr-debug",
            "request": "launch",
            "input": "test.gogo",
            "grammar": "GoGo.g4",
            "startRule": "prog",
            "printParseTree": true,
            "visualParseTree": true
        },
        {
            "name": "antlr4-GoGo2",
            "type": "antlr-debug",
            "request": "launch",
            "input": "test2.gogo",
            "grammar": "GoGo.g4",
            "startRule": "prog",
            "printParseTree": true,
            "visualParseTree": true
        }
    ]
}